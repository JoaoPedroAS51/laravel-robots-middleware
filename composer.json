{
    "name": "joaopedroas51/laravel-robots-middleware",
    "description": "Add an `all` or `none` robots header to your requests via a middleware in Laravel",
    "keywords": [
        "robots",
        "spatie",
        "seo",
        "index",
        "robots"
    ],
    "homepage": "https://github.com/JoaoPedroAS51/laravel-robots-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "illuminate/http": "^5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\RobotsMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\RobotsMiddleware\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
